      <input id="txt-search-bar" type="text" class="form-control input-lg" placeholder="type the victim name">
    </form>
    <ul id="hunt-list">
      <% for(var idx = 0; idx < 5; idx++){ %>
      <li>
        <img class="photo" />
        <div class="detail">
          <h3 class="name">John Doe</h3>
          <span class="follower">1000 followers</span>
        </div>
        <a href="" class="mark-trigger">MARK</a>
      </li>
      <% } %>
    </ul>
  </div></div>
</section>
<% include master_footer %>
